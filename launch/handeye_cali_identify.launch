<?xml version="1.0" ?>
<launch>
    <!-- franka initialization -->
    <include file="$(find franka_handeye_cali)/launch/handeye_init.launch"/>

    <!-- realsense initialization -->
    <include file="$(find realsense2_camera)/launch/rs_camera.launch" >
        <arg name="align_depth" value="true" />
        <arg name="filters" value="pointcloud" />
    </include>

    <!-- perform eye-in-hand calibration -->
    <node name="gui_handeye_cali_node" pkg="franka_handeye_cali" type="gui_handeye_cali.py" args="" required="true" output="screen" />

</launch>
